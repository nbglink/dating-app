{
  "name": "dating-app-monorepo",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "scripts": {
    "dev:web": "turbo run dev --filter=web",
    "dev:mobile": "turbo run dev --filter=mobile",
    "dev:mobile:clear": "turbo run dev --filter=mobile -- --clear",
    "dev:api": "turbo run dev --filter=api",
    "gen:sdk": "turbo run gen:sdk --filter=@dating/api-client",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck"
  },
  "devDependencies": {
    "turbo": "2.5.8"
  }
}
